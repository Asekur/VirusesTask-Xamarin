<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="VirusTask.Themes.DarkTheme">

    <Color x:Key="BackgroundColor">#E1E1E1</Color>
    <Color x:Key="TextColor">#2B2B2B</Color>
    <Color x:Key="BorderColor">#2B2B2B</Color>
    <Color x:Key="PhotoBackground">#FFFFFF</Color>
    <Color x:Key="TintColor">#2B2B2B</Color>
    <Color x:Key="ThumbColor">#2B2B2B</Color>
    <Color x:Key="MinimumTrackColor">#2B2B2B</Color>
    <Color x:Key="MaximumTrackColor">#2B2B2B</Color>
    <Color x:Key="FrameColor">#FF1f1f1f</Color>
    <Color x:Key="TextPrimaryColor">#B0FFFFFF</Color>
    <Color x:Key="TextSecondaryColor">#eFeFeF</Color>

    <Style x:Key="Title" TargetType="Label">
        <Setter Property="TextColor" Value="{StaticResource TextPrimaryColor}"/>
        <Setter Property="FontAttributes" Value="Bold"/>
        <Setter Property="FontSize" Value="Large"/>
    </Style>
</ResourceDictionary>
